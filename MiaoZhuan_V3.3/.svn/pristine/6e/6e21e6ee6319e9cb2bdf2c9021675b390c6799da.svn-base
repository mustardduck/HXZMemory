#import "ReturnSucceedViewController.h"

@interface ReturnSucceedViewController ()
@property (retain, nonatomic) IBOutlet UILabel *theStatement;

@end

@implementation ReturnSucceedViewController
@synthesize titleString = _titleString;

MTA_viewDidAppear()
MTA_viewDidDisappear()
- (void)viewDidLoad {
    [super viewDidLoad];
    [self setTitle:@"协商退货"];
    [self setupMoveBackButton];
    self.theStatement.text = _titleString;
}

- (void)didReceiveMemoryWarning {
    [super didReceiveMemoryWarning];
}

- (void)dealloc {
    [_titleString release];
    [_theStatement release];
    [super dealloc];
}
@end
